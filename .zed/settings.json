{
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "cargo": {
          "allTargets": false,
          "target": "thumbv6m-none-eabi"
        },
        "check": {
          "command": "clippy"
        }
      }
    }
  },
  "languages": {
    "Rust": {
      "format_on_save": "on",
      "formatter": "auto",
      "inlay_hints": {
        "enabled": true
      }
    }
  }
}
