{
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "check": {
          "command": "clippy"
        }
      }
    }
  },
  "languages": {
    "Rust": {
      "format_on_save": "on",
      "formatter": "auto",
      "inlay_hints": {
        "enabled": true
      }
    }
  }
}
